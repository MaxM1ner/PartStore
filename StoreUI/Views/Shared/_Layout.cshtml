<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - PartStore</title>
    <!-- fonts -->
    <link rel="preconnect" href="https://fonts.googleapis.com" />
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin />
    <link
      href="https://fonts.googleapis.com/css2?family=Montserrat:ital,wght@0,400;0,500;0,600;0,700;1,700&display=swap"
      rel="stylesheet"
    />
    <!-- styles -->
    @* <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/StoreUI.styles.css" asp-append-version="true" />*@
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
</head>
<body id="animation">
    
    <header class="header">
    <div class="containerr">
    <div class="header-inner">
                <a class="logo" asp-area="" asp-controller="Home" asp-action="Index"> <span class="logo-span">Part</span><span class="logo-span2">Store</span></a>
        <nav class="header-nav">
                    <ul class="nav-list">         
                        <li class="nav-item">
                            <a class="nav-link " asp-area="" asp-controller="Home" asp-action="Catalog">Catalog</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-controller="PcBuilder" asp-action="Index">PC Builder </a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link " asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link " asp-area="" asp-controller="Home" asp-action="Cart">Cart</a>
                        </li>
                    </ul>
        </nav>
        <partial name="_LoginPartial" />
    </div>
    </div>
    </header>

    <main role="main" class=" main">
        
            @RenderBody()
       
        </main>
   

    <footer class="footer">
     <div class="section ">
        <div class="containerr">
            <div class="footer-content">
                    <h2 class="section-title"> pc is our need, it is a tool for creating a great... <br /> <span class="translucent">or you can just play games)</span></h2>
            <div class="footer-contacts">
            <p>Contact us</p>
                        <ul class="fooder-list">
                            <li> <a href="tel:+380969999999">+380 (96) 999-99-99</a></li>
                            <li><a href="mailto:dsdsfdfdsfsd@gmail.com">partstore@gmail.com</a></li>
            </ul>
            </div>
            </div>

            @*&copy; 2023 - PartStore - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>*@
        </div>
        </div>
    </footer>
    
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    @* <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>*@
    <script src="~/js/site.js" asp-append-version="true"></script>
    <script src="~/js/three.min.js"></script>
    <script src="~/js/vanta.net.min.js"></script>
    <script src="~/js/app.js"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
